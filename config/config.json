{
  "development": {
    "dialect": "sqlite",
    "storage": "db.sqlite",
    "logging": false,
    "dialectOptions": {}
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  }
}
